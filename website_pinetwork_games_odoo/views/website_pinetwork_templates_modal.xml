        <template id="latinchain_mainnet_redirect" name="Modal View">
            <t t-call="web.layout">
                &lt;!DOCTYPE html&gt;
                <html>
                    <head>
                        <meta charset="UTF-8"/>
                        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
                        <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
                        <title>LatinChain X.</title>
                        <meta name="apple-mobile-web-app-capable" content="yes" />
                        <meta http-equiv="X-UA-Compatible" content="ie=edge"/>
                        <meta name="viewport" content="width=device-width, user-scalable=yes"/>
                        
                        <!-- ios support -->
                        <meta name="apple-mobile-web-app-status-bar" content="#db4938" />
                        <meta name="theme-color" content="#db4938" />
                        
                        <meta name="application-name" content="LatinChain X."/>
                        
                        <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
                        
                        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous"/>
                        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
                        
                        <script t-if="mainnet in ['Testnet ON', 'Testnet OFF']" async="true" t-att-src="google_adsense" crossorigin="anonymous"></script>
                        
                        <script type="text/javascript" t-if="mainnet in ['Testnet ON', 'Testnet OFF']">
                          window._taboola = window._taboola || [];
                          _taboola.push({article:'auto'});
                          !function (e, f, u, i) {
                            if (!document.getElementById(i)){
                              e.async = 1;
                              e.src = u;
                              e.id = i;
                              f.parentNode.insertBefore(e, f);
                            }
                          }(document.createElement('script'),
                          document.getElementsByTagName('script')[0],
                          '//cdn.taboola.com/libtrc/latinchain-publisher/loader.js',
                          'tb_loader_script');
                          if(window.performance &amp;&amp; typeof window.performance.mark == 'function')
                            {window.performance.mark('tbl_ic');}
                        </script>
                        
                    </head>

                    <body>
                        <div class="container form-group" style="text-align:center; max-width: 810px">
                            <br/>
                            <div style="font-size: 95%">
                                We are testing LatinChain Mainnet on Pi Network cryptocurrency ecosystem. Get double utility usage bonus <i class="fa-solid fa-bolt"></i> by using both Mainnet and Testnet.
                                <br/>
                                <br/>
                            </div>
                            <a href="/mainnet" target="_blank" class="btn btn-success mb-2 o_default_snippet_text"><strong><div>Click here</div><div>(Go to LatinChain Mainnet)</div></strong></a>
                            <br/>
                            <br/>
                            <a href="javascript:void(0);" onclick="parent.jQuery.colorbox.close()">Return to LatinChain Testnet</a>
                            <br/>
                            <br/>
                            <div class="container" style="text-align:center;" t-if="mainnet in ['Testnet ON', 'Testnet OFF']">
                                <!-- LatinChain Ad responsive 2 -->
                                
                                <ins class="adsbygoogle"
                                     style="display:block"
                                     data-ad-client="ca-pub-2334449220276386"
                                     data-ad-slot="9525388880"
                                     data-ad-format="auto"
                                     data-full-width-responsive="true"></ins>
                                <script>
                                     (adsbygoogle = window.adsbygoogle || []).push({});
                                </script>
                                
                                <br/>
                                
                                <div id="taboola-below-article-thumbnails"></div>
                                <script type="text/javascript">
                                  window._taboola = window._taboola || [];
                                  _taboola.push({
                                    mode: 'alternating-thumbnails-a',
                                    container: 'taboola-below-article-thumbnails',
                                    placement: 'Below Article Thumbnails',
                                    target_type: 'mix'
                                  });
                                </script>
                            </div>
                        </div>
                        <br/><br/><br/><br/><br/>
                        
                        <button id="myBtn"><a id="myABtn" href="#top" style="color: white" translate="no">Top</a></button>
                    
                        <style>
                            /*Floating Back-To-Top Button*/
                                #myBtn {
                                    position: fixed;
                                    bottom: 10px;
                                    float: right;
                                    right: 10%;
                                    bottom: 10%;
                                    max-width: 50px;
                                    max-height: 50px;
                                    width: 100%;
                                    height: 100%;
                                    font-size: 18px;
                                    border-color: rgba(85, 85, 85, 0.2);
                                    background-color: rgb(100,100,100);
                                    /*padding: 10px;*/
                                    border-radius: 4px;
                                    opacity: 0.5;
                                    z-index:100;
                                }
                            /*On Hover Color Change*/
                                #myBtn:hover {
                                    background-color: #7dbbf1;
                                }
                        </style>
                        
                        <script type="text/javascript" t-if="mainnet in ['Testnet ON', 'Testnet OFF']">
                          window._taboola = window._taboola || [];
                          _taboola.push({flush: true});
                        </script>
                    </body>
                </html>
            </t>
		</template>
        
